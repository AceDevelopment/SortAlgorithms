    }
    
    @BeforeClass
    public static void setUpClass() {
    }
    
    @AfterClass
    public static void tearDownClass() {
    }
    
    @Before
    public void setUp() {
    }
    
    @After
    public void tearDown() {
    }

  
    @Test
    public void insertionSort(){

        assertArrayEquals(new int [] {1,2,3}, new int [] {1,2,3});
        
    }
    
    @Test
    public void bubbleSort(){

        assertArrayEquals(new int [] {1,2,3}, new int [] {1,2,3});
        
    }
    @Test
    public void selectionSort(){

        assertArrayEquals(new int [] {1,2,3}, new int [] {1,2,3});
        
    }
